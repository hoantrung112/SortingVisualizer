<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane fx:id="scenePane" prefHeight="665.0" prefWidth="487.0" style="-fx-background-color: transparent;" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller">
    <children>
        <AnchorPane fx:id="backScene1" prefHeight="89.0" prefWidth="541.0" style="-fx-background-color: #3d4956;">
            <children>
                <ImageView fx:id="exitButtonImage" fitHeight="21.0" fitWidth="26.0" layoutX="506.0" layoutY="13.0" onMousePressed="#exitMenu" pickOnBounds="true" preserveRatio="true">
                    <image>
                        <Image url="@images/settings.png" />
                    </image>
                </ImageView>
                <Label layoutX="14.0" layoutY="23.0" prefHeight="43.0" prefWidth="444.0" text="Network Problem Visualization Program" textAlignment="CENTER" textFill="WHITE" wrapText="true">
                    <font>
                        <Font name="System Bold Italic" size="22.0" />
                    </font>
                </Label>
            </children>
        </AnchorPane>
        <AnchorPane fx:id="backScene2" layoutY="99.0" prefHeight="567.0" prefWidth="541.0" style="-fx-background-color: #3d4956;">
            <children>
                <Label layoutX="15.0" layoutY="14.0" prefHeight="111.0" prefWidth="493.0" text="This is the network generation visualization (Presented by group 9). Please choose an option below so the application will visualize it for you!" textAlignment="JUSTIFY" textFill="WHITE" wrapText="true">
                    <font>
                        <Font name="System Italic" size="21.0" />
                    </font>
                </Label>
                <Button fx:id="MS" layoutX="35.0" layoutY="160.0" mnemonicParsing="false" onAction="#MSScene" prefHeight="78.0" prefWidth="210.0" text="Merge Sort" />
                <Button fx:id="HS" layoutX="35.0" layoutY="270.0" mnemonicParsing="false" onAction="#HSscene" prefHeight="78.0" prefWidth="210.0" text="Heap Sort" />
                <Button fx:id="BS" layoutX="35.0" layoutY="380.0" mnemonicParsing="false" onAction="#BSscene" prefHeight="78.0" prefWidth="210.0" text="Bucket Sort" />
                <Button fx:id="RS" layoutX="271.0" layoutY="160.0" mnemonicParsing="false" onAction="#RSscene" prefHeight="78.0" prefWidth="210.0" text="Radix Sort" />
            <Button fx:id="QS" layoutX="271.0" layoutY="270.0" mnemonicParsing="false" onAction="#QSscene" prefHeight="78.0" prefWidth="210.0" text="Quick Sort" />
            <Button fx:id="BuS" layoutX="271.0" layoutY="380.0" mnemonicParsing="false" onAction="#BuSscene" prefHeight="78.0" prefWidth="210.0" text="Bubble Sort" />
            </children></AnchorPane>
    </children>
</AnchorPane>
